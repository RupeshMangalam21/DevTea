# DevTea Production Environment
# Use these as reference for production deployment

# Google OAuth (Production)
GOOGLE_CLIENT_ID=123456789-prodclientid.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=GOCSPX-productionsecret

# Redis (Production)
REDIS_URL=rediss://default:ProductionPassword@prod-redis.upstash.io:6380

# JWT Secret (Production - Different from dev)
JWT_SECRET=production_jwt_secret_64_characters_long_and_very_secure_string

# CORS (Production)
ALLOWED_ORIGINS=https://devtea-chat.vercel.app,https://chat.devtea.com

# Sentry (Production)
SENTRY_DSN=https://prodkey@o123456.ingest.sentry.io/654321

# NextAuth (Production)
NEXTAUTH_URL=https://devtea-chat.vercel.app
NEXTAUTH_SECRET=production-nextauth-secret

# Production Settings
NODE_ENV=production
DEBUG=false
MOCK_GOOGLE_AUTH=false

# Database (When migrating to persistent storage)
DATABASE_URL=postgresql://user:pass@prod-db.neon.tech:5432/devtea_prod

# Performance
RATE_LIMIT_MAX=60
WS_TIMEOUT=30000
MAX_MESSAGE_LENGTH=1000
